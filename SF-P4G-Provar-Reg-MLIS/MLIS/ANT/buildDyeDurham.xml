<project default="runtests">
	<property environment="env"/>
	<property name="provar.home" value="${env.PROVAR_HOME}"/>
	<property name="testproject.home" value="${env.TEST_PROJECT_HOME}"/>
	<property name="testproject.results" value="${testproject.home}/ANT/Results"/>
	<!--<property name="provar.home" value="C:/Program Files/Provar2.9.0/"/>
		<property name="testproject.home" value=".."/>
		<property name="testproject.results" value="../ANT/Results"/>-->
	<property name="secrets.password" value="${env.ProvarSecretsPassword}"/>
	<property name="testenvironment.secretspassword" value="${env.ProvarSecretsPassword_EnvName}"/>
	<property name="environmentvar" value="${env.ENVIRONMENT}"/>
	<property name="testcycle.path" value="${testproject.home}/ANT/Cycle"/>
	
	<taskdef name="Provar-Compile" classname="com.provar.testrunner.ant.CompileTask" classpath="${provar.home}/ant/ant-provar.jar"/>
	<taskdef name="Run-Test-Case" classname="com.provar.testrunner.ant.RunnerTask" classpath="${provar.home}/ant/ant-provar.jar;${provar.home}/ant/ant-provar-bundled.jar;${provar.home}/ant/ant-provar-sf.jar"/>
	<taskdef name="Test-Cycle-Report" classname="com.provar.testrunner.ant.TestCycleReportTask" classpath="${provar.home}/ant/ant-provar.jar;${provar.home}/ant/ant-provar-bundled.jar;${provar.home}/ant/ant-provar-sf.jar"/>
	
	<target name="runtests">
	
		<Provar-Compile provarHome="${provar.home}" projectPath="${testproject.home}"/>
	
		<Run-Test-Case provarHome="${provar.home}" 
				projectPath="${testproject.home}" 
				smtpPath="${testproject.home}/.smtp/"
			licensePath="${testproject.home}/.licenses"
				resultsPath="${testproject.results}" 
				resultsPathDisposition="Increment" 
				testEnvironment="${environmentvar}" 
				webBrowser="Chrome" 
				webBrowserConfiguration="Full Screen"
				webBrowserProviderName="Desktop"
				webBrowserDeviceName="Full Screen" 
				excludeCallableTestCases="false" 
				salesforceMetadataCache="Reuse" 
				projectCachePath="../../.provarCaches"
				testOutputlevel="BASIC" 
				pluginOutputlevel="WARNING"
				stopTestRunOnError="false"
				secretsPassword="${secrets.password}"
				testEnvironmentSecretsPassword="${testenvironment.secretspassword}"
				testCyclePath="${testcycle.path}"
			   	testCycleRunType="ALL"
				dontFailBuild="true"
				invokeTestRunMonitor="false"
				>
			
			
			<fileset dir="../tests/86024_DT-DFMLIS-E2E"><include name="90921_DT-DFMLIS-E2E_Trigger_Referral__Broker_Give_LOI_More_than_5000000_for_Residential_DyeDurham.testcase"/></fileset>
			<fileset dir="../tests/86024_DT-DFMLIS-E2E"><include name="91151_DT-DFMLIS-E2E_Create_Residential_Quote_(More_than_One_Risk)_DyeDurham.testcase"/></fileset>
			<fileset dir="../tests/86024_DT-DFMLIS-E2E"><include name="91152_DT-DFMLIS-E2E_Create Commercial Quote (One product)_DyeDurham.testcase"/></fileset>
			<fileset dir="../tests/86024_DT-DFMLIS-E2E"><include name="91153_DT-DFMLIS-E2E_Verify_user_able_to_create_a_Single_product_commercial_policy_from_DyeDurham_Brk_UW.testcase"/></fileset>
			<fileset dir="../tests/86024_DT-DFMLIS-E2E"><include name="91154_DFMLIS-E2E_Create_DyeDurham_Residential_Quote_Multiple Product_Brk_UW.testcase"/></fileset>
			<fileset dir="../tests/86024_DT-DFMLIS-E2E"><include name="91160_DT-DFMLIS-E2E_Residential_Quick_Quote_(House_Buyer_or_Seller)_DyeDurham.testcase"/></fileset>
			<fileset dir="../tests/86024_DT-DFMLIS-E2E"><include name="91404_DT-DFMLIS-E2E_Verify_Default_UW_User_able_to_validate_the_Sfiles_DyeDurham_Res.testcase"/></fileset>	
			
			
			
		</Run-Test-Case>
		
		<Run-Test-Case provarHome="${provar.home}" 
						projectPath="${testproject.home}" 
						smtpPath="${testproject.home}/.smtp/"
			licensePath="${testproject.home}/.licenses"
						resultsPath="${testproject.results}" 
						resultsPathDisposition="Increment" 
						testEnvironment="${environmentvar}" 
						webBrowser="Chrome" 
						webBrowserConfiguration="Full Screen"
						webBrowserProviderName="Desktop"
						webBrowserDeviceName="Full Screen" 
						excludeCallableTestCases="false" 
						salesforceMetadataCache="Reuse" 
						projectCachePath="../../.provarCaches"
						testOutputlevel="BASIC" 
						pluginOutputlevel="WARNING"
						stopTestRunOnError="false"
						secretsPassword="${secrets.password}"
						testEnvironmentSecretsPassword="${testenvironment.secretspassword}"
						testCyclePath="${testcycle.path}"
					   	testCycleRunType="FAILURES_AND_SKIPPED"
						dontFailBuild="true"
						invokeTestRunMonitor="false"
						>
				
			
						<fileset dir="../tests/86024_DT-DFMLIS-E2E"><include name="90921_DT-DFMLIS-E2E_Trigger_Referral__Broker_Give_LOI_More_than_5000000_for_Residential_DyeDurham.testcase"/></fileset>
						<fileset dir="../tests/86024_DT-DFMLIS-E2E"><include name="91151_DT-DFMLIS-E2E_Create_Residential_Quote_(More_than_One_Risk)_DyeDurham.testcase"/></fileset>
						<fileset dir="../tests/86024_DT-DFMLIS-E2E"><include name="91152_DT-DFMLIS-E2E_Create Commercial Quote (One product)_DyeDurham.testcase"/></fileset>
						<fileset dir="../tests/86024_DT-DFMLIS-E2E"><include name="91153_DT-DFMLIS-E2E_Verify_user_able_to_create_a_Single_product_commercial_policy_from_DyeDurham_Brk_UW.testcase"/></fileset>
						<fileset dir="../tests/86024_DT-DFMLIS-E2E"><include name="91154_DFMLIS-E2E_Create_DyeDurham_Residential_Quote_Multiple Product_Brk_UW.testcase"/></fileset>
						<fileset dir="../tests/86024_DT-DFMLIS-E2E"><include name="91160_DT-DFMLIS-E2E_Residential_Quick_Quote_(House_Buyer_or_Seller)_DyeDurham.testcase"/></fileset>
						<fileset dir="../tests/86024_DT-DFMLIS-E2E"><include name="91404_DT-DFMLIS-E2E_Verify_Default_UW_User_able_to_validate_the_Sfiles_DyeDurham_Res.testcase"/></fileset>	
								
				</Run-Test-Case>
		<Test-Cycle-Report provarHome="${provar.home}"
						projectPath="${testproject.home}" 
			        	testCyclePath="${testcycle.path}"
						smtpPath="${testproject.home}/.smtp/"
						reportPath="${testcycle.path}/ReRunConsolidatedReport.pdf"
						failBuildIfFailures="true"
						invokeTestRunMonitor="true"
						>
					<emailProperties sendEmail="true" primaryRecipients="shankar.krishnarao@dualgroup.com,subhasis.mahanty@dualgroup.com" ccRecipients="" bccRecipients="" emailSubject="Provar test run report - MLIS-DyeDurham Regression" attachExecutionReport="true" attachZip="false"/>
								<attachmentProperties includeAllFailuresInSummary="true" includeBdd="true" includeSkipped="false" includeTestCasePathHierarchy="true" includeTestCaseShowSummary="true" includeBasicLogs="true" includeDetailLogs="true" includeDiagnosticTrace="false" includeTestStepTime="true" includeNoScreenshot="false" includeScreenshotThumbnail="false" includeFullScreenShot="true"/>
				</Test-Cycle-Report>
	</target>
</project>
